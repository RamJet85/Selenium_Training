<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Cross Browser Testing Suite" verbose ="1">
  
  <test name="HubSpot Test">
  <parameter name ="browser" value="chrome" />
	<classes>
		<class name="Parallel_Testing.HubSpotTest" />
	</classes>
    </test>
       
	<test name="BoxLogin Test">
	<parameter name ="browser" value="firefox" />
		<classes>
			<class name="Parallel_Testing.BoxLogin" />
		</classes>
	</test>
	
	<test name="PimsTest Test">
	<parameter name ="browser" value="ie" />
		<classes>
			<class name="Parallel_Testing.PimsTest" />
		</classes>
	</test> 
</suite> 
